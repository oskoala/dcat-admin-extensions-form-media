{
    "name": "oskoala/form-media",
    "alias": "form-media",
    "description": "Dcat-admin image/video input extension.",
    "type": "library",
    "keywords": [
        "dcat-admin",
        "lake",
        "media",
        "form-media",
        "extension"
    ],
    "homepage": "https://github.com/deatil/dcat-form-media",
    "license": "MIT",
    "authors": [
        {
            "name": "deatil",
            "email": "deatil@github.com",
            "homepage": "https://github.com/deatil"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "dcat/laravel-admin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Lake\\FormMedia\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "dcat-admin": "Lake\\FormMedia\\ServiceProvider",
        "laravel": {
            "providers": [
                "Lake\\FormMedia\\ServiceProvider"
            ]
        }
    }
}
